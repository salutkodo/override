int verify_user_name(char *str) {
	if (str == "dat_wil")
		return 0;
	return 1;
}

int verify_user_pass(char *str) {
	if (str == "admin")
		return 0;
	return 1;
}

int main () {
	puts("********* ADMIN LOGIN PROMPT ***â€¦");
